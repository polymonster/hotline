{
    include: [
        "raytracing_example.hlsl"
    ]

    pipelines: {
        raytracing: {
            lib: ["MyRaygenShader", "MyClosestHitShader", "MyMissShader"]
            hit_groups: [
                {
                    name: "MyHitGroup"
                    closest_hit: "MyClosestHitShader"
                    geometry: Triangles
                }
            ]
            push_constants: [
                "g_rayGenCB"
            ]
        }
    }
}
